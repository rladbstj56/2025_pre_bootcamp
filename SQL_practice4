use basic;

create table basic.sparta_students(
	id int auto_increment primary key,
	name varchar(10),
	track varchar(10),
	grade varchar(4),
	enrollment_year int
	)

select *
from sparta_students;

insert into sparta_students (name, track, grade, enrollment_year)
values 
	('르탄이','Node.js','A',2023),
	('배캠이', 'Spring', 'B', 2022),
	('구구이', 'Unity', 'C', 2021),
	('이션이','Node.js','B',2022);

select name, track
from sparta_students;

select *
from sparta_students
where track <> 'Unity';

select *
from sparta_students
where enrollment_year in (2021,2023)

select enrollment_year
from sparta_students ss 
where track = 'Node.js' and grade = 'A';
